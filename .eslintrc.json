{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [0, "never"],
    "arrow-body-style": 0,
    "no-unused-vars": 1,
    "dot-notation": 1,
    "no-trailing-spaces": 0,
    "arrow-parens": 0,
    "no-plusplus": 0,
    "react/jsx-no-bind": 0,
    "prefer-const": 0,
    "react/forbid-prop-types": 0,
    "radix": 0,
    "object-shorthand": 1,
    "no-prototype-builtins": 1,
    "no-restricted-syntax": 1,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 1
  }
}